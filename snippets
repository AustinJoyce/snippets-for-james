                        fr.setInputForm(eqip.getFormName() + " " + eqip.getFormVersion());
                        fr.setSubjectName(person.getFirstName() + " " + person.getLastName());
                        fr.setSubjectSSN(Integer.parseInt(person.getSsn()));
